package com;

public class StringBasics {

	public static void main(String[] args) {
		String s1 ="abc";
		String s2 = "abc";
		if(s1==s2) {
			System.out.println("s1==s2");
		}
         System.out.println("s1 hashcode :-"+s1.hashCode());
         System.out.println("s2 hashcode :-"+s2.hashCode());

	}

}
